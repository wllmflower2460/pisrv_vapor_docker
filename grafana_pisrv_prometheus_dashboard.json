{
  "dashboard": {
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "type": "timeseries",
        "title": "Request Rate (req/s) by Route/Method",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "editorMode": "code",
            "expr": "sum by (route, method)(rate(http_requests_total{route=~\"$route\",method=~\"$method\"}[$__rate_interval]))",
            "legendFormat": "{{route}} {{method}} {{status}}",
            "range": true,
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Error Rate (%)",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "editorMode": "code",
            "expr": "(sum(rate(http_requests_total{status=~\"5..|4..\",route=~\"$route\",method=~\"$method\"}[$__rate_interval]))) / ignoring(status) (sum(rate(http_requests_total{route=~\"$route\",method=~\"$method\"}[$__rate_interval]))) * 100",
            "legendFormat": "error%",
            "range": true,
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Latency Quantiles (s) by Route",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "histogram_quantile(0.50, sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[$__rate_interval])))",
            "legendFormat": "{{route}} p50",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "histogram_quantile(0.90, sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[$__rate_interval])))",
            "legendFormat": "{{route}} p90",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "histogram_quantile(0.99, sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[$__rate_interval])))",
            "legendFormat": "{{route}} p99",
            "refId": "C"
          }
        ]
      },
      {
        "type": "table",
        "title": "Top Routes (req/s, errors, avg latency)",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {}
            }
          }
        ],
        "fieldConfig": {
          "defaults": {},
          "overrides": []
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "sum by (route)(rate(http_requests_total{route=~\"$route\"}[$__rate_interval]))",
            "legendFormat": "{{route}} req/s",
            "refId": "A",
            "format": "table"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "sum by (route)(rate(http_requests_total{status=~\"5..|4..\",route=~\"$route\"}[$__rate_interval]))",
            "legendFormat": "{{route}} err/s",
            "refId": "B",
            "format": "table"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "sum by (route)(rate(http_request_duration_seconds_sum{route=~\"$route\"}[$__rate_interval])) / sum by (route)(rate(http_request_duration_seconds_count{route=~\"$route\"}[$__rate_interval]))",
            "legendFormat": "{{route}} avg_s",
            "refId": "C",
            "format": "table"
          }
        ]
      },
      {
        "type": "heatmap",
        "title": "Status Codes Heatmap",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "show": true
          },
          "tooltip": {
            "show": true
          }
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "sum by (status) (rate(http_requests_total{route=~\"$route\",method=~\"$method\"}[$__rate_interval]))",
            "refId": "A"
          }
        ]
      }
    ],
